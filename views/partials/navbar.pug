nav.navbar.navbar-expand-lg.navbar-dark.myNav-home.fixed-top
  a.navbar-brand.link-my-brand(href="#my")
    i.fa.fa-star(aria-hidden="true") &nbsp; m .
  button.navbar-toggler(
    type="button"
    data-toggle="collapse"
    data-target="#navbarSupportedContent"
    aria-controls="navbarSupportedContent"
    aria-expanded="false"
    aria-label="Toggle navigation")
      span.navbar-toggler-icon
  div#navbarSupportedContent.collapse.navbar-collapse
    ul.navbar-nav.mr-auto
      each entry in helpers.navigation
        li.nav-item
          a.link-my.nav-link(
          data-scroll=`${entry.dataScroll}`
          href=`${entry.href}`
          class=(currentPath === entry.href ? 'active' : '')
          ) #{entry.name}